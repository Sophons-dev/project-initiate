// Organization Models
// This file contains all organization-related models

model Organization {
  id          String           @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  type        OrganizationType
  description String?
  website     String?
  logoUrl     String?
  location    String?
  organizationUrl String?
  createdAt   DateTime?
  updatedAt   DateTime?
  opportunities Opportunity[]
}
